  rdfs:label "name" ;
.
alps:Synchronization
  a owl:Class ;
  a sh:NodeShape ;
  rdfs:subClassOf alps:NonBranching ;
.
alps:Task
  a owl:Class ;
  a sh:NodeShape ;
  rdfs:subClassOf alps:NonBranching ;
  sh:property [
      a sh:PropertyShape ;
      sh:path alps:hasDuration ;
