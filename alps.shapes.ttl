  a sh:NodeShape ;
  rdfs:label "name" ;
  rdfs:subClassOf alps:SingleSuccessor ;
  sh:not [
      rdfs:label "Instance of name" ;
      sh:class alps:Join ;
    ] ;
  sh:not [
      rdfs:label "Instance of name" ;
      sh:class alps:Join ;
    ] ;
  sh:not [
      rdfs:label "Instance of name" ;
      sh:class alps:NonBranching ;
    ] ;
  sh:not [
      rdfs:label "Instance of name" ;
      sh:class alps:NonBranching ;
    ] ;
.
alps:StartPlan_1
